<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <style>
      .container {
        width: 60%;
        text-align: center;
        margin-top: 3%;
      }
    </style>
    <title>Hasil Screening Covid-19 Kelurahan Binar Selatan</title>
  </head>
  <body>
    <div class="container">
      <h1>Hasil Screening Covid-19 Kelurahan Binar Selatan</h1>
      <table class="table table-bordered table-hover">
        <thead class="table-dark">
          <tr>
            <th>Nama</th>
            <th>Alamat</th>
            <th>Demam</th>
            <th>Nyeri Tenggorokan</th>
            <th>Sesak Nafas</th>
            <th>Batuk</th>
          </tr>
        </thead>
        <tbody>
          <% peserta.forEach((user) => { %>
          <tr>
            <td><%= user.namaLengkap %></td>
            <td><%= user.alamat %></td>
            <td><%= user.demam %></td>
            <td><%= user.tenggorokan%></td>
            <td><%= user.nafas %></td>
            <td><%= user.batuk %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </body>
</html>
